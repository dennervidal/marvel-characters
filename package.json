{
  "name": "marvel-characters",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fontsource/pt-sans": "^4.5.10",
    "@fontsource/pt-sans-caption": "^4.5.10",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@material-ui/styles": "^4.11.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "prestart": "yarn install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "eslint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "eslint:fix:dry": "eslint ./src --ext .ts,.tsx --fix-dry-run",
    "eslint:fix:quiet": "eslint ./src --ext .ts,.tsx --fix --quiet",
    "prettify": "prettier --write src/**/*.{json,ts,tsx} && prettier --write *.{json,ts,tsx}",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "yarn eslint:fix:quiet",
      "yarn prettify",
      "git add ./src"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.2"
  },
  "repository": {
    "type": "git",
    "directory": ".",
    "url": "git+ssh://git@github.com/dennervidal/marvel-characters.git"
  },
  "bugs": {
    "url": "https://github.com/dennervidal/marvel-characters/issues"
  },
  "homepage": "https://dennervidal.github.io/marvel-characters/"
}
