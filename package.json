{
  "name": "marvel-characters",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@material-ui/styles": "^4.11.5",
    "@fontsource/pt-sans": "^4.5.10",
    "@fontsource/pt-sans-caption": "^4.5.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "prestart": "yarn install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "prettify": "prettier --write src/**/*.{js,json} && prettier --write *.{js,json}",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint ./src --ext .js --fix",
      "yarn prettify",
      "git add ."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^8.0.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1"
  },
  "repository": {
    "type": "git",
    "directory": ".",
    "url": "git+ssh://git@github.com/dennervidal/marvel-characters.git"
  },
  "bugs": {
    "url": "https://github.com/dennervidal/marvel-characters/issues"
  },
  "homepage": "https://dennervidal.github.io/marvel-characters/"
}
